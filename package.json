{
  "name": "file-caching",
  "version": "0.0.0",
  "description": "File caching system",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "lint": "npm-run-all lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint ./src/",
    "lint-fix": "npm run lint:eslint -- --fix && prettier --write .",
    "precommit": "npm-run-all precommit:*",
    "precommit:eslint": "pretty-quick --check",
    "precommit:prettier": "npm run lint:eslint",
    "precommit:test": "npm run test:unit",
    "precommit:typecheck": "npm run typecheck",
    "test:cc": "jest --clearCache",
    "test:unit": "jest -c jest.config.json --detectOpenHandles --forceExit",
    "test:watch": "jest -c jest.config.json --watch --silent false",
    "test:coverage": "npm run test:unit -- --ci --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bovidiu/node-file-caching.git"
  },
  "keywords": [
    "node",
    "caching",
    "caching",
    "caching",
    "node",
    "file",
    "caching",
    "node",
    "file",
    "caching"
  ],
  "author": "Ovidiu Bokar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bovidiu/node-file-caching/issues"
  },
  "homepage": "https://github.com/bovidiu/node-file-caching#readme",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.1.0",
    "typedoc": "^0.20.35",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "wepack": "0.0.1"
  }
}
